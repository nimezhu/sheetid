{
  "name": "SheetID",
  "version": "0.0.1",
  "description": "Get Google SheetID",
  "permissions": ["tabs"],
  "icons": {
    "16": "images/favicon-16x16.png",
    "32": "images/favicon-32x32.png",
    "96": "images/favicon-96x96.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/favicon-16x16.png",
      "32": "images/favicon-32x32.png",
      "96": "images/favicon-96x96.png"
    }
  },
  "web_accessible_resources": [

  ],
  "manifest_version": 2
}
